{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**/*", "package.json"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/index.ts": {
            "maxDuration": 30
        }
    }
}